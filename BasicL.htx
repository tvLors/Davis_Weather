     <script>       
       var mytxt = "<!--wind10Avg-->";
       document.write(mytxt.slice(1, mytxt.length-2));
     </script>
       </strong></d2><l2><strong>&nbsp;MPH</strong></l2></font></p></td>
-->
   <td colspan="7" bordercolor="#000000" style="white-space:nowrap; text-align:center; border: solid rgb(0,0,0)"><font face="verdana, Arial, 
       Helvetica"><t1><strong><!--outsideTemp-->&deg;</strong></t1></font></td>
 </tr>
 <tr bgcolor="#FFF899">
   <td colspan="1" style="text-align:right; border-right: none; border-left: solid rgb(0,0,0); border-top: solid rgb(0,0,0); border-bottom: solid rgb(0,0,0)">
