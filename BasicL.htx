<table border="3" bordercolor="#000000" style="border-top: none" cellpadding="0" cellspacing="0">
 <tr bgcolor="#000000">
   <td colspan="1" style="width:90px; text-align:center"><div style="height: 2px; bgcolor="#000000"><z1>1</z1></div></td>
   <td colspan="1" style="width:134px; text-align:center"><div style="height: 2px; bgcolor="#000000"><z1>2</z1></div></td>
   <td colspan="1" style="width:74px; text-align:center"><div style="height: 2px; bgcolor="#000000"><z1>3</z1></div></td>
   <td colspan="1" style="width:84px; text-align:center"><div style="height: 2px; bgcolor="#000000"><z1>4</z1></div></td>
   <td colspan="1" style="width:35px; text-align:center"><div style="height: 2px; bgcolor="#000000"><z1>5</z1></div></td>
   <td colspan="1" style="width:133px; text-align:center"><div style="height: 2px; bgcolor="#000000"><z1>6</z1></div></td>
   <td colspan="1" style="width:65px; text-align:center"><div style="height: 2px; bgcolor="#000000"><z1>7</z1></div></td>
   <td colspan="1" style="width:24px; text-align:center"><div style="height: 2px; bgcolor="#000000"><z1>8</z1></div></td>
